<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel ui:field="panel">
        <span>
            <h2 class="displayInline">
                <g:Anchor ui:field="title"/>
            </h2>
            <g:Anchor ui:field="upGroupLink"/> &#160;&#160;
            <g:Anchor ui:field="geneGroupLink"/>  &#160;&#160;
            <g:InlineLabel ui:field="uniquePeptideCount"/>
        </span>
        <p>
            <!--<g:InlineLabel ui:field="description"/><br/>-->
            <g:InlineLabel ui:field="altId"/><br/>
            <g:InlineLabel ui:field="name"/><br/>
            <g:InlineLabel ui:field="species"/><br/>
            <g:InlineLabel ui:field="geneSymbol"/><br/>
            <g:InlineLabel ui:field="proteinEvidence"/>
        </p>
        <p>
            <g:FlowPanel ui:field="attributes"/>
        </p>
    </g:HTMLPanel>
</ui:UiBinder>